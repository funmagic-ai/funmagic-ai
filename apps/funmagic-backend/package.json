    "@funmagic/shared": "workspace:*",
    "@hono/zod-openapi": "^1.2.0",
    "bullmq": "^5.67.2",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.11.7",
    "ioredis": "^5.9.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.9.3"
  }
